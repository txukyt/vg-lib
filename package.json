{
  "name": "@txukyt/j38-lib",
  "version": "1.1.1",
  "description": "Librería compartida de utilidades para proyectos web del Ayuntamiento de Vitoria-Gasteiz",
  "type": "module",
  "main": "dist/j38-lib.iife.js",
  "module": "dist/j38-lib.esm.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/j38-lib.iife.js"
    },
    "./components/header": {
      "import": "./dist/esm/components/header/index.js",
      "require": "./dist/j38-lib.iife.js"
    }
  },
  "scripts": {
    "clean": "rmdir /s /q dist",
    "lint": "eslint .",
    "build": "cross-env NODE_ENV=dev rollup -c ",
    "build:prod": "cross-env NODE_ENV=prod rollup -c",
    "prebuild": "npm run lint",
    "postbuild": "npm run docs",
    "deploy:dev": "node scripts/deploy.js dev",
    "deploy:desa": "node scripts/deploy.js desa",
    "deploy:pre": "node scripts/deploy.js pre",
    "deploy:prod": "node scripts/deploy.js prod",
    "docs": "jsdoc -c jsdoc.json"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "vitoria-gasteiz",
    "vg",
    "librería",
    "web",
    "ui"
  ],
  "author": "Ayuntamiento de Vitoria-Gasteiz",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "clean-jsdoc-theme": "^4.3.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jsdoc": "^4.0.4",
    "rollup": "^4.50.1"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-replace": "^6.0.2",
    "copy-paste": "^2.2.0",
    "i18next": "^25.3.2",
    "shelljs": "^0.10.0"
  },
  "overrides": {
    "@rollup/plugin-eslint": {
      "eslint": "^9.35.0"
    }
  }
}
