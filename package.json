{
  "name": "@vitoria-gasteiz/vg-lib",
  "version": "1.0.5",
  "description": "Librería compartida de utilidades para proyectos web del Ayuntamiento de Vitoria-Gasteiz",
  "type": "module",
  "main": "dist/vg-lib.iife.js",
  "module": "dist/vg-lib.esm.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/vg-lib.iife.js"
    },
    "./components/header.js": {
      "import": "./dist/esm/components/header.js",
      "require": "./dist/vg-lib.iife.js"
    }
  },
  "scripts": {
    "clean": "rmdir /s /q dist",
    "build": "rollup -c",
    "deploy:dev": "node scripts/deploy.js dev",
    "deploy:desa": "node scripts/deploy.js desa",
    "deploy:pre": "node scripts/deploy.js pre",
    "deploy:prod": "node scripts/deploy.js prod",
    "docs": "jsdoc -c jsdoc.json"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "vitoria-gasteiz",
    "vg",
    "librería",
    "web",
    "ui"
  ],
  "author": "Ayuntamiento de Vitoria-Gasteiz",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "dotenv": "^17.2.0",
    "jsdoc": "^4.0.4",
    "rollup": "^3.0.0"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-replace": "^6.0.2",
    "copy-paste": "^2.2.0",
    "i18next": "^25.3.2",
    "shelljs": "^0.10.0"
  }
}
