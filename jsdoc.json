{
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "readme": "./README.md",
        "destination": "./dist/docs",
        "recurse": true,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "homepageTitle": "Documentaci√≥n de j38-lib ",
            "default_theme": "dark",
            "menu": [
                {
                    "title": "README",
                    "link": "index.html"
                }
            ]
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}